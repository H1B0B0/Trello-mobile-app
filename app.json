{
  "expo": {
    "name": "Trellow",
    "scheme": "trellow",
    "slug": "Trellow",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-light.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "package": "com.hibobo.Trellow"
    },
    "plugins": ["expo-secure-store"],
    "extra": {
      "eas": {
        "projectId": "3f7becfc-e415-41a9-bd55-90adc73f927f"
      }
    }
  }
}
